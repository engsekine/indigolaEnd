//src ディレクトリ作成

mkdir src

//src/html ディレクトリ作成
mkdir src/html && touch src/html/index.html

//src/css src/scss ディレクトリ作成
mkdir src/css && touch src/css/style.css && mkdir src/scss && touch src/scss/style.scss

//src/js ディレクトリ作成
mkdir src/js && touch src/js/index.js


まとめたやつ
mkdir src && mkdir src/html && touch src/html/index.html && mkdir src/css && touch src/css/style.css && mkdir src/scss && touch src/scss/style.scss mkdir src/js && touch src/js/index.js